{
    "auto-rename-tag.activationOnLanguage": [
        "javascriptreact",
        "typescriptreact"
    ],
    "breadcrumbs.symbolPath": "off",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "Fira Code, 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.rulers": [
        120
    ],
    "editor.smoothScrolling": true,
    "editor.tabCompletion": "on",
    "editor.tabSize": 2,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "files.associations": {
        "*.js": "javascriptreact"
    },
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/CVS": true,
        ".cache/": true,
        ".idea/": true,
        "node_modules": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "javascript.preferences.quoteStyle": "single",
    "materialTheme.accent": "Remove accents",
    "materialTheme.autoApplyIcons": false,
    "search.smartCase": true,
    "terminal.external.linuxExec": "konsole",
    "typescript.preferences.quoteStyle": "single",
    "window.menuBarVisibility": "toggle",
    "window.restoreWindows": "folders",
    "window.zoomLevel": 0.4,
    "workbench.colorCustomizations": {},
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "workbench.commandPalette.history": 80,
    "workbench.editor.labelFormat": "short",
    "workbench.sideBar.location": "left",
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "workbench.statusBar.feedback.visible": false,
    "workbench.view.alwaysShowHeaderActions": true
}
